{
  "name": "angular-webpack-starter",
  "version": "0.2.0",
  "description": "Angular Webpack Starter",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node webpack.server.js",
    "test": "npm run test-eslint && npm run test-karma",
    "test-eslint": "cross-env NODE_ENV=test eslint src",
    "test-karma": "cross-env NODE_ENV=test karma start test/karma.conf.js",
    "test:watch": "npm run test -- --no-single-run",
    "build": "rimraf dist/*; cross-env NODE_ENV=production webpack"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.5.0-rc.1",
    "bootstrap": "^3.3.6"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.8",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-class-properties": "^6.4.0",
    "babel-plugin-transform-function-bind": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015-loose": "^7.0.0",
    "browser-sync": "^2.11.1",
    "chai": "^3.4.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-mgcrea": "^1.0.0",
    "eslint-loader": "^1.2.0",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.0",
    "karma": "^0.13.19",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.5",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "ng-annotate-loader": "^0.1.0",
    "rimraf": "^2.5.0",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.11",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  }
}
